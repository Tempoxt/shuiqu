/* pages/orderDetail/orderDetail.wxss */
.swiper-tab{ 
  width: 100%; 
  text-align: center; 
  height: 40px;
  line-height: 40px;
  position: relative;
  z-index: 1000;
} 
.swiper-tab-item{ 
  font-size: 30rpx; 
  display: inline-block; 
  height: 38px;
  width: 50%; 
  color: #4d555d;
  /* box-sizing: border-box;   */
  /* border-bottom: 2px solid transparent;  */
} 
.on{
  color: #000; 
  font-weight: bold; 
  border-bottom: 2px solid #F8D32F;
} 
 
.swiper-box{ display: block; height: 100%; width: 100%; overflow: hidden;background-color: #eee; } 
.swiper-box .logistics{
    margin-top:30rpx;
    height:100%;
    display:flex;
}
.swiper-box .logistics .logistics-progress{
    width:120rpx;
    text-align:center;
    position:relative;
}
.swiper-box .logistics .logistics-progress .line-gray{
    width:1rpx;
    height:100%;
    background:#ccc;
    position:absolute;
    left:50rpx;
}
.swiper-box .logistics .text-tips{
    height:100%;
    flex:1;
}
.swiper-box .logistics .text-tips .item-window{
    display:flex;
    align-items:center;
    width:97%;
    background:#fff;
    height:120rpx;
    margin:30rpx 2% 30rpx 0;
    position:relative;
    border-radius:2rpx;
}

.swiper-box .logistics .text-tips .item-window:first-child{
   margin:0 2% 30rpx 0%;
}
.swiper-box .logistics .text-tips .item-window .msg{
    font-size:28rpx;
    padding-left:10rpx;
}
.swiper-box .logistics .text-tips .item-window .little-arrow{
    position:absolute;
    width:0;
    height:0;
    border-top:20rpx solid transparent;
    border-bottom:20rpx solid transparent;
    border-right:30rpx solid #fff;
    left:-20rpx;
    top:50%;
    margin-top:-20rpx;
}
.swiper-box .logistics .text-tips .item-window .phone-icon{
    width:60rpx;
    height:60rpx;
    background:#F8D52F;
    border-radius:50%;
    position:absolute;
    left:-96rpx;
    z-index:101;
}
.swiper-box .logistics .text-tips .item-window .line-yellow{
    width:1rpx;
    height:100%;
    background:#F8D52F;
    position:absolute;
    left:-69rpx;
    padding-bottom:30rpx;
    z-index:100;
}
