/* pages/couponDetail/couponDetail.wxss */
.discount-wrap{
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    padding-bottom: 20rpx;
    margin: 20rpx 20rpx 0 20rpx;
}
.discount-info{
    margin: 0 20rpx;
}
.discount-info view{
    display: flex;
    height: 60rpx;
    justify-content: space-between;
    align-items: center;
    font-size: 26rpx;
    padding: 0 20rpx;
    border-top: 1rpx solid #e0e0e0;
}