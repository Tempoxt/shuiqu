/* pages/unauthorized/unauthorized.wxss */
page{
    height: 100%;
    background: #eee;
}
.container{
    
}
.container .tips{
    display: flex;
    padding: 80rpx 40rpx;
    flex-direction: column;
    justify-content: center; 
    align-items: center; 
    background: #fff;
}
.container .tips image{
    width:200rpx;
    height: 200rpx;
}
.container .tips .title{
    font-size: 34rpx;
    margin: 40rpx 0 20rpx 0;
    color: #656666;
}
.container .tips .go-empower{
    font-size: 30rpx;
    margin-bottom: 20rpx;
    color: #656666;
}
.container .tips .warn-tips{
    font-size: 24rpx;
    color: #A3A3A3;
}
.container .empower{
    padding: 0 40rpx;
}
.container .empower .title{
    font-size: 34rpx;
    padding: 20rpx 0;
    color: #767677;
}
.container .empower .step-one,.container .empower .step-two{
    font-size: 30rpx;
    color: #9E9E9E;
}
.container .empower .step-one .delLittleApp{
    display: flex;
    justify-content: center;
}
.container .empower .step-one .delLittleApp image{
    width: 460rpx;
    height: 216rpx;
    padding: 50rpx 0;
}
.container .empower .step-two .shouquan{
    display: flex;
    justify-content: center;
}
.container .empower .step-two .shouquan image{
    width: 460rpx;
    height: 392rpx;
    padding: 50rpx 0;
}
