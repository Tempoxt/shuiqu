    width:100%;
    height:100rpx;
    position:relative;
    
}
.container .footer view{
    width: 100%;
    position: absolute;
    bottom: 10rpx;
    left: 0;
    display:flex;
    align-items:center;
    justify-content:center;
    font-size:24rpx;
} */
.container .footer view image{
    width:55rpx;
    height:55rpx;
}
.bind-bg{
    width:100%;
    height:100%;
    position:absolute;
    display:flex;
    align-items:center;
    justify-content:center;
    z-index:10;
    top:0;
    background: rgba(0,0,0,0.3);
}
.bind-bg .bind-box {
    width:510rpx;
    height:336rpx;
    display: flex;
    flex-direction: column;
    align-content: center;
    position:relative;
    background:#fff;
    box-shadow: 4px 4px 6px 0 rgba(185,185,185,0.3);
}
.bind-bg .bind-box .title{
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 32rpx;
    margin-top: 32rpx;
}
.bind-bg .bind-box button{
    display: flex;
    justify-content: center;
    align-items: center;
    width:344rpx;
    height: 72rpx;
    font-size: 26rpx;
    border-radius: 8rpx;
    margin-top: 32rpx;
}
.bind-bg .bind-box button::after {
  border: 0;
} 
.bind-bg .bind-box .weixin-bind{
    color: #fff;
    background: #3CAF00;
}
.bind-bg .bind-box .weixin-bind image{
    width:48rpx;
    height:48rpx;
    position:absolute;
    left:10rpx;
}
.bind-bg .bind-box .phone-bind{
    color:#333333;
    background: #EFEFEF;
    
}
.bind-bg .bind-box .phone-bind image{
    width:48rpx;
    height:48rpx;
    position:absolute;
    left:12rpx;
}
.shade{
    width:100%;
    height:100%;
    position:absolute;
    top:0;
}
