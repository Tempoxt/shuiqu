.modalWrap{
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0;
    background-color: rgba(0, 0, 0, 0.3); 
    z-index: 100;
}
.bg-blur{
      /* filter:blur(6px);  */
}
.none{
    display: none;
}
.modalWrap .modal{
    /* display: flex; */
    margin: 0 50rpx;
    /* flex-direction: column; */
    /* width: 80%;
    margin-left: 10%; */
    width:650rpx;
    box-sizing: border-box;
    height: 740rpx;
    background: #fff; 
    position: absolute;
    top: 20%;
    border-radius:30rpx;
    box-shadow: 0 4px 6px 0 rgba(185,185,185,0.3);
}
.modalWrap .modal .header{
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    border-bottom: 1rpx solid #eee;
    
}
.modalWrap .modal .header .title{
    font-weight: bold;
}
.modalWrap .modal .header .choose-location{
    position: absolute;
    display: flex;
    align-items: center;
    font-size: 26rpx;
    width: 150rpx;
    padding: 0 10rpx;
}
.modalWrap .modal .header .choose-location .city{
    width: 110rpx;
    text-align: center;  
    overflow: hidden;
    white-space: nowrap;
    text-overflow : ellipsis;
}
.modalWrap .modal .header .choose-location .arrow{
    width:30rpx;
    height:30rpx;
    margin-right: 20rpx; 
}
.modalWrap .modal .header .title{
    font-size: 32rpx;
    color: #393636;
}
.modalWrap .modal .header .close{
    width: 30rpx;
    height: 30rpx;
    /* line-height: 60rpx; */
    padding:25rpx 20rpx;
    /* font-weight: normal; */
    position: absolute;
    right: 0rpx;
    /* top: 25rpx; */
}
.modalWrap .modal .inputs{
    margin:60rpx 5% 30rpx 5%;
    font-size:30rpx;
}
.modalWrap .modal .inputs view{
    display: flex;
    align-items: center;
    height: 80rpx;
    border-radius: 80rpx;
    border:1rpx solid #e0e0e0;
    padding-left:10rpx;
}
.modalWrap .modal .inputs view {
    margin-bottom: 24rpx;
}
.modalWrap .modal .inputs view input{
    height: 80rpx;
    padding-left: 16rpx; 
}
.modalWrap .modal .inputs .validate .getCode{
    width: 160rpx;
    height: 80rpx;
    line-height: 80rpx;
    padding: 0;
    border:0 none;
    border-radius: 0 80rpx 80rpx 0;
    font-size: 24rpx;
    color: #fff;
}
.modalWrap .modal .inputs .validate .theme1{
    background: linear-gradient(to right,#F6CC47,#FFAD52);
}
.modalWrap .modal .inputs .validate .theme2{
    background: linear-gradient(to right,#7b00d3,#b25aff);
}
.modalWrap .modal .inputs .validate .theme3{
    background: linear-gradient(to right,#cf1731,#ff4060);
}
.modalWrap .modal .inputs .validate .theme4{
    background: linear-gradient(to right,#15a0ff,#3ebaff);
}
.modalWrap .modal .inputs .validate .theme5{
    background: linear-gradient(to right,#2cc136,#5de04f);
}
.modalWrap .modal .inputs .validate .getCode::after{
    border:0 none;
}
.modalWrap .modal .inputs .validate input{
    flex: 1;
    padding-left: 16rpx;
}
.modalWrap .modal .agree{
    font-size:26rpx;
    margin:0 5%;
    height:40rpx;
    display: flex;
    align-items: center;
    margin-bottom: 24rpx;
}
.modalWrap .modal .agree image{
    width:40rpx;
    height:40rpx;
    padding-right: 10rpx;
}
.modalWrap .modal .agree view text{
    color:#0070C0;
    /* margin-left:10rpx; */
}
.modalWrap .modal .register-tips-box{
    display: flex;
    flex-direction: column;
    font-size: 26rpx;
    margin: 0 5%;
    color: #666666;
}
.modalWrap .modal .register-tips-box .title{
    margin-bottom: 10rpx;
    font-size: 30rpx;
    color: #000;
    font-weight: bold;
}
.modalWrap .modal .register-tips-box .list-item{
    margin-bottom: 5rpx;
}
.modalWrap .modal .commit{
    background:#FAD600;
    height:80rpx;
    line-height:80rpx;
    padding:0;
    margin:0 5%;
    font-size:30rpx;
    position:absolute;
    width:90%;
    bottom:20rpx;
    border-radius: 80rpx;
}
.modalWrap .modal .commit::after{
    border: 0;
}
.city-list .city-box{
    margin:0 20rpx 20rpx 20rpx;
}
.city-list .city-box .title-list-wrap .title{
    font-size: 32rpx;
}
.city-list .city-box .list-hot-city{
    margin:20rpx;
}
.city-list .city-box .list-hot-city text{
    border: 1rpx solid #E0E0E0;
    display: inline-block;
    border-radius: 10rpx;
    margin: 10rpx;
    color: #B4B4B4;
    padding: 6rpx 12rpx;
    font-size:30rpx;
}
.city-list .city-box .list-city{
    margin:0 20rpx 20rpx 20rpx;
}
.city-list .city-box .list-city .first-letter{
    display:block;
    width: 100%;
    height:60rpx;
    line-height: 60rpx;
    text-indent:20rpx;
}
.city-list .city-box .list-city .city-name{
    border: 1rpx solid #E0E0E0;
    display: inline-block;
    border-radius: 10rpx;
    margin: 10rpx;
    color: #B4B4B4;
    font-size:30rpx;
    padding: 6rpx 12rpx;
}

.city-list image{
    width:50rpx;
    height:50rpx;
    position:absolute;
    top: 0rpx;
    right:20rpx;
}
.city-show{
    display: block;
    background:#fff;
    width:100%;
    height:100%;
    position:absolute;
    top:0;
    z-index: 100;
}
.city-hide{
    display: none;
}