/* pages/ticketDetail/ticketDetail.wxss */
page{
    height: 100%;
}
.container{
    width:100%;
    height: 100%;
    background-color: #eee;
    font-size:30rpx;
}
.container .header{
    background: #fff;
    padding: 20rpx 0 20rpx 30rpx;
    border-bottom:1rpx solid #eee;
}
.container .detail{
    background: #fff;
    padding: 20rpx 0 20rpx 30rpx;
    margin-bottom: 20rpx;
}
.container .other .title{
    width:100%;
    text-align:center;
}
.container .other .title view{
    border-bottom:1rpx solid #eee;
}
.container .other .info{
    width:100%;
    text-align:center;
}
.container .other .title view,.container .other .info view{
    width:50%;
    padding:10rpx 0;
    background-color: #fff;
    float:left;
}
