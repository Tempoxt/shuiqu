.commodity{
    font-size:0.7rem;
    h3{
         background: #F5F5F5;
         color: #616161;
         padding: 0 0.5rem;
         font-weight: normal;
         border-top: 1px solid #E2E2E2;
         line-height: 2rem;
        
    }
    .store-list{
        padding-bottom: 3rem;
        &>li{
            position: relative;
            .noscope{
                width: 30%;
                position: absolute;
                top: 5%;
                right: 5%;
                display: none;
            }
            background: #fff;
            margin-bottom: 0.5rem;
            .h4{
                width: 100%;
                tr td{
                    vertical-align: middle;
                    padding: 0.2rem 0.5rem;
                    border-bottom: 1px solid #E9E9E9;
                    &:first-child{
                        line-height: 0em;
                        padding-right: 0;
                        width: 10%;
                    }
                }
                
                img{
                    display: inline-block;
                    width: 2rem;
                }
             
            }
            .store-cont{
                tr td{
                    padding: 0.5rem;
                    &.bot-box{
                        background: #f9f9f9;
                        padding: 0.2rem 0.5rem;
                    }
                    &:first-child{
                        width: 20%;
                        img{
                            display: block;
                            width: 100%;
                        }
                    }
                    h5{
                        line-height: 1.5rem;
                    }
