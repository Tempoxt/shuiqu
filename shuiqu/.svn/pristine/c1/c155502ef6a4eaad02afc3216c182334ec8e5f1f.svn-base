@charset "utf-8";
/*基础数据，可配置*/
@import "src/base";
/*icon图标*/
@import "src/icon";

.public-color{
    background: #F9D600!important;
}
.icon-color{
    color: #F9D600;
}
.version-color{
    background-color: #F9D600;
}
.public-icon-color{
    color: #FDC312;
}
.right-margin{
    margin-right: .5rem;
}
.gray-color{
    background: #ccc!important;
}
.bar-nav{
    background: $mainBgColor;
    border:0;
}
.bar-nav1{
    background: $mainBgColor;
    color: #000;
    border:0;
}
.bar-left{
    position: absolute!important;
    left: 0;
    right: auto;
}
.light-nav{
    background: #FDFDFD;
}
.price{
    color: $lightColor;
}
.pick-nav{
    background: #F7F7F8;
}
.required{
    color: $red;
}
.err-msg{
    color: $red;
    line-height: 1.2rem;
    font-size: 0.65rem;
    text-indent: 1rem;
}
/*绑定手机号*/
.page-bind{
    .content{
        background-color: #fff;
        .logo{
            text-align: center;
            img{
                width:2.5rem;
                height:2.5rem;
                margin: 2rem 0;
            }
        }
        .inputs{
            border-top:1px solid #eee;
            border-bottom:1px solid #eee;
            font-size: 0.8rem;
            .username{
                height:2rem;
                /*line-height: 2rem;*/
                border-bottom:1px solid #eee;
                label{
                    margin-left: 0.5rem;
                }
                .cellphone{
                    border:0;
                    height:100%;
                }
                
            }
            .password{
                height:2rem;
                position: relative;
                label{
                    margin-left: 0.5rem;
                }
                .code{
                    border:0;
                    height: 100%;
                }
                #getCode{
                    width: 4rem;
                    padding: 0;
                    height:1.5rem;
                    position: absolute;
                    top:0.25rem;
                    right:0.5rem;
                    font-size: 0.6rem;
                    background-color: #F9D600;
                    outline: 0;
                    border: 0;
                }
            }
        }
        .agree-notice{
            height: 2rem;
            line-height: 2rem;
            font-size: 0.6rem;
            margin-left: 0.5rem;
            a{
                color: #3879D9;
            }
        }
        .submit-wrap{
            height:1.6rem;
            line-height:1.6rem;
            margin: 1.5rem 0.5rem 0 0.5rem;
            text-align: center;
            border-radius: 0.2rem;
            #submit{
                width: 100%;
                height:100%;
                font-size:0.8rem;
                background-color: #eee;
                border:0;
                outline: 0;
            }
        }
    }
}
/*绑定表单样式*/
.reg-list{
$regLiHeight: 2rem;
    ul{
        width: 96%;
        margin: 0 auto;
        li{
            margin: 1rem 0 0;
            .agree-note{
                color:#0070C0;
                text-decoration: underline;
            }
        }
        li.input{
            position: relative;
            width: 100%;
            border: 1px solid #606060;
            background: #fff;
            line-height: $regLiHeight;
            label{
                padding: 0 0.5rem 0 0.3rem;
                height: $regLiHeight;
            }
            input{
                border: none;
                background: none;
                height: $regLiHeight;
                line-height: $regLiHeight;
            }
            span{
                height: 100%;
                width: 6rem;
                position: absolute;
                display: block;
                top: 0;
                right: 0;
                background: #aaa;
                text-align: center;
                color: #fff;
                input{
                    text-align: center;
                    width: 100%;
                    height: 100%;
                }
                .no-bg-input{
                    color: #3D4145;
                }
            }
        } 
        .bind-text{
            text-align: center;
        }
    }
    .button.button-fill{
        width: 96%;
        margin: 0 auto;
        line-height: $regLiHeight;
        height: $regLiHeight;
        font-size: 0.8rem;
    }
    
    p{
        margin-top: 0.8rem;
        input.btn-gray{
            color: #3D4145;
            background: #AAAAAA;
        }
    }
}
/*底部导航条*/

.bar-tab{
    .tab-item.active{
        color: #000;
    }
    .tab-item .def,
    .tab-item.active .cur{
        display: block;
        margin: 0 auto;
    }
    .tab-item .cur,
    .tab-item.active .def{
        display: none;
    }
}

/*bar-footer-secondary & order-bar*/
.order-bar,
.bar-footer-secondary{
    z-index:100;
    table{
        width: 100%;
        height: 100%;
        color: #BABABA;
        font-size: 0.6rem;
        span{
            font-weight: bold;
            color: $lightColor;
            font-size: 0.8rem;
        }
        .go-buy{
            width: 5rem;
            .button{
                top: -0.1rem;
            }
        }
        .add-shopCart{
            width: 5rem;
            .button{
                top: -0.1rem;
            }
        }
    }
    
}

/*闪订列表*/
$carBorderColor:#F5F5F5;
.car-list{
    padding-bottom: 4rem;
    li{
        background: #fff;
        border-bottom: 1px solid #CDCDCD;
        margin-bottom: 1rem;
        h3{
            position: relative;
            font-size: 0.75rem;
            line-height: 2rem;
            height: 2rem;
            padding-left: 0.5rem;
            color: #575757;
            a{
                font-weight: normal;
                font-size: 0.6rem;
                width: 3rem;
                position: absolute;
                right: 0.5rem;
                top: 0.3rem;
            }
        }
        .car-cont{
            padding: 0 0.5rem;
            table{
                border-top: 1px solid $carBorderColor;
                font-size: 0.8rem;
                tr td{
                    padding: 0.3rem 0;
                }
                .edit-box{
                    position: relative;
                    .icon-11addressdel{
                        bottom:1.4rem;
                    }
                }
                td:nth-child(2){
                    width:25%;
                    padding-right: 2%;
                }
                td:nth-child(3){
                    width:65%;
                }
                img{
                    display: block;
                    width: 100%;
                }
                h4{
                    color: #575757;
                    font-size: 0.65rem;
                    font-weight: normal;
                }
                .price{
                    font-weight: bold;
                    color: $lightColor;
                }
                .no-money{
                    font-size: 0.65rem;
                    text-decoration: line-through;
                    font-weight: normal;
                }
                p{
                    color: #BEBEBE;

                    span{
                        font-size: 0.7rem;
                        display: inline-block;
                        padding-right: 0.3rem;
                    }
                }
                .car-count{
                    font-size: 1.2rem;
                    line-height: 1.2rem;
                    i{
                        font-size: 1rem;
                        font-style: normal;
                        padding: 0 0.5rem;
                    }
                }
                .car-btn span{
                    font-size: 1.3rem;
                    margin-right: 0.5rem;
                 }
                em{
                    font-style: normal;
                    color: $lightColor;
                }
            }
        }
    }
    
}
/*首页*/
$homeGap: 0.5rem;
.top-nav{
    padding:0 0.5rem;
    .searchbar{
        background: none;
        width: 65%;
        float: right;
        padding-left: 0;
        .search-input{
            a{
                display: block;
                label{
                    left: auto;
                    right: 0;
                }
                #search{
                    padding-left: 1rem;
                }
            }
        }
    }
      
    span{
        display:block;
        float: left;
        overflow: hidden;
        text-overflow: ellipsis;
        white-space: nowrap;
        background: #987F08;
        color: #fff;
        border-radius: 0.2rem;
        height: 1.4rem;
        line-height: 1.4rem;
        padding: 0 0.2rem;
        width: 35%;
        text-align: center;
        margin: 0.4rem 0 0 0;
    }
}
#bar{
    background-color:transparent; 
}
#bar span{
    background-color: rgba(0,0,0,0.3);
}
#bar:after{
    height:0;
}
.swiper-container{
    padding-bottom: 0;
   .swiper-slide img{
        width: 100%;
    } 
}
.swiper-container-horizontal{
    overflow: hidden;
}
.swiper-container-horizontal > .swiper-pagination{
    bottom: 0.2rem;
}
.notice-box{
    /*background: #fff;*/
    line-height: 2em;
    height: 2em;
    font-size: 0.8rem;
    color: #7D889E;
    padding: 0 $homeGap;
    em{
        font-style: normal;
        color: red;
        font-weight: bold;
    }
}
.home-wrap{
    padding: $homeGap $homeGap 5rem;
    .fresh{
        width: 100%;
        margin-bottom: $homeGap;
        overflow: hidden;
        li{
            width: 48%;
            float: left;
            background: #fff;
            &:first-child{
                margin-right:4%;
            }
            table{
                tr td{
                    p{
                        font-size: 0.6rem;
                        line-height: 1.5em;
                    }
                    &:first-child{
                        width: 40%;
                        padding-right: 5%;
                    }
                   
                    svg{
                        display: block;
                        width: 60%;
                        margin-left:25%;
                    }
                    h4{
                        font-size: 0.8rem;
                        line-height: 1.5em;
                    }
                    
                }
            } 
            
            p{
                color: #C29A1E;
                
            }
            
        }
    }
    .defaut-wrap{
        padding:0rem 1rem 1rem;
        color:#b7b8b7;
        text-align: center;
        p{
            width:9rem;
            margin:0 auto;
        }
        .def-note{
            font-size:0.8rem;
            text-align:center;
            font-weight: normal;
        }
        .icon{
            display:block;
            font-size:3rem;
            width:3rem;
            height:3rem;
            margin:0 auto;}
        
    }
}
.company{
    $cmpStaCor: #fff;
    $cmpEndCor: #E7E7E7;
    li{
        border: #E3E3E3 1px solid;
        margin-bottom: $homeGap;
        background: -webkit-linear-gradient(left, $cmpStaCor 45%, $cmpEndCor); /* Safari 5.1 - 6.0 */
        background: -o-linear-gradient(right, $cmpStaCor 45%, $cmpEndCor); /* Opera 11.1 - 12.0 */
        background: -moz-linear-gradient(right, $cmpStaCor 45%, $cmpEndCor); /* Firefox 3.6 - 15 */
        background: linear-gradient(to right, $cmpStaCor 45%, $cmpEndCor); /* 标准的语法 */
        table tr td{
            width: 36%;
            padding: 0 2%;
            &:first-child{
                width: 22%;
            }
            h4{
                font-size: 0.8rem;
                line-height: 2em;
            }
            p{
                font-size: 0.65rem;
                color: #5B5B5B;
                height: 3em;
                overflow: hidden;
            }
            img{
                display: block;
                width: 100%;
            }
        }
    }
}
/*搜索品牌或电话*/
.page-searchShopOrProduct{
    .light-nav{
        padding: 0;
        div.searchbar{
            padding: 0;
            height: 2.2rem;
            line-height: 2.2rem;
            margin: 0;
            width: 70%;
            .search-input{
                height: 2.2rem;
                #search{
                    height: 2.2rem;
                    padding: 0;
                }
            }
        }
    }

}
/*扫码结果页*/
.popup-validate{
    .content-block{
        .content{
            .validate-banner{
                img{
                    width: 100%;
                }
            }
            .validate-content{
                .validate-status{
                    text-align: center;
                    font-size: 2rem;
                }
                .tips{
                    font-size: 0.8rem;
                    text-indent: 2em;
                    padding: 0 2rem;
                    span{
                        color: #F5D236;
                    }
                    .first-query{
                        color: #f00;
                    }
                }
                .modal{
                    top: 40%;
                    width: 85%;
                    left: 0;
                    margin-left: 7.5%;
                    .modal-inner{
                        background: #F9D700;
                        border:2px solid #D4D6D2;
                        border-radius:0.35rem;
                        text-align: left;
                        .modal-text{
                            .close-win{
                                position: absolute;
                                right: -0.6rem;
                                top: -0.8rem;
                                font-size: 1.5rem;
                                width: 1.5rem;
                                height: 1.5rem;
                                line-height: 1.5rem;
                                border-radius: 50%;
                            }
                        }
                    }
                }
                .validate-detail{
                    width:14rem;
                    font-size:0.65rem;
                    padding:1rem;
                    position:fixed;
                    top:50%;
                    left:50%;
                    margin-left:-7rem;
                    z-index:101;
                    margin-top:-2rem;
                    background:#F9D600;
                    display:none;
                    .close-win{
                        position: absolute;
                        right: 0;
                        top:-0.5rem;
                        font-size: 1rem;
                        cursor:pointer;
                    }
                }
            }
            .show-picture{
                text-align: center;
                img{
                    width: 40%;
                }
            }
            .show-detail{
                margin:0 auto;
                width:5rem;
                height:1.5rem;
                line-height: 1.5rem;
                font-size: 0.8rem;
                padding: 0;
            }
        }
        .content.content-query-fail{
            text-align: center;
            i{
                display:block;
                font-size: 4rem;
                margin-top:8rem;
            }
            span{
                color:#A3A6B0;
                font-size:0.8rem;
            }
        }
    }
}
/*兑换优惠码*/
.img-code-box{
    background: #fff;
    .img-box img{
        width: 100%;
        display: block;
    }
}
.img-code-form {
    ul li.input{
        border-color: #bcbcbc;
        border-radius: 0.4rem;
    }
    .notice{
        color: #666;
        h5{
            font-weight: normal;
            font-size: 0.8rem;
            padding-bottom: 0.5rem;
        }
        p{
            margin: 0;
            font-size: 0.7rem;
            line-height: 1.8em;
            text-indent: 1rem;
        }
    } 

    
}


/*搜索页*/
.search-bar{
    i{
        max-width: 75%;
        overflow: hidden;
        display: inline-block;
        height: 2.2rem;
        line-height: 2.2rem;
        font-style: normal;
    }
    span{
        display: block;
        width: 10%;
        &.choose-location{
            margin-left: 2rem;
            width: 25%;
            overflow: hidden;
            font-size: 0.7rem;
            padding: 0;
        }
        .icon-63navbaricoarrowdown{
            color: #666;
            float: right; 
            font-size: 0.7rem;
        }
    }
    .icon-111icosearch{
        display: inline-block;
        padding: 0;
        position: absolute;
        right: 0;
        top: 0;
    }
    .searchbar{
        background: none;
        width: 80%;
        &.location-search{
            width: 60%;
        }
    }
}
.show-list{
    background: #fff;
    a{
        color: #AFAFAF;
    }
    li{
        padding: 0.5rem ;
    }
    h4{
        color: #000;
        font-size: 0.8rem;
    }
    p{
        font-size: 0.6rem;
        white-space:nowrap; 
        overflow:hidden; 
        text-overflow:ellipsis;
    }
    img{
        display: block;
        height: 4rem;
        width: 100%;
    }
    table tr td{
        &:first-child{
            width:35%;
            padding-right: 0.3rem;
        }
    }
}
.has-border li{
    border-bottom: 1px solid #EBEBEB;
}
/*定位页*/
.city-box{
    padding-top: 1.5em;
}
.city-content{
    background-color: #eee;
}
.title-list-wrap{
    font-size: 0.7rem;
    h4{
        /*background: #F5F5F5;*/
        border-top: 1px solid #E9E9E9;
        border-bottom: 1px solid #E9E9E9;
        color: #7C7C7C;
        padding: 0 0.5rem;
        line-height: 1.5rem;
        font-weight: normal;

    }
    .now-location{
        background-color: #fff;
        td{
            vertical-align: middle;
            padding: 0.5rem;
            &:last-child{
                width:10%;
                font-size: 1.5rem;
            }
        }
        
    }
    .my-address{
        padding: 0.5rem;
        background-color: #fff;
        border-bottom: 1px solid #eee;
    }
    .my-history{
        padding: 0.5rem;
        background-color: #fff;
        border-bottom: 1px solid #eee;
    }
    p{
        text-align: center;
        height: 2rem;
        line-height: 2rem;
    }
}
.list-city{
    padding: 0 0.5rem;
    a{

        line-height: 1.5em;
        border: 1px solid #CDCCD0;
        border-radius: 0.2rem;
        display: inline-block;
        margin: 0.2rem;
        padding: 0.1rem 0.3rem;
        
    }
    h5{
        display: block;
        font-size: 0.8rem;
        width: 100%;
        height: 3em;
        line-height: 3em;
        text-indent: 0.5rem;
    }
}
.list-hot-city{
    padding: 0.5rem;
    a{
        line-height: 1.5em;
        border: 2px solid #E0E0E0;
        display: inline-block;
        border-radius: 0.2rem;
        margin: 0.2rem;
        color: #B4B4B4;
        padding: 0.1rem 0.3rem;
    }
}
