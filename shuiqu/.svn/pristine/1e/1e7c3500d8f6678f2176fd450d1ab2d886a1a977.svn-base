/*底部导航条*/
.bar-tab .tab-item.active {
  color: #000; }
.bar-tab .tab-item .def,
.bar-tab .tab-item.active .cur {
  display: block;
  margin: 0 auto; }
.bar-tab .tab-item .cur,
.bar-tab .tab-item.active .def {
  display: none; }

/*首页*/
.top-nav {
  padding: 0 0.5rem; }
  .top-nav .searchbar {
    background: none;
    width: 65%;
    float: right;
    padding-left: 0; }
    .top-nav .searchbar .search-input a {
      display: block; }
      .top-nav .searchbar .search-input a label {
        left: auto;
        right: 0; }
      .top-nav .searchbar .search-input a #search {
        padding-left: 1rem; }
  .top-nav span {
    display: block;
    float: left;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    background: #987F08;
    color: #fff;
    border-radius: 0.2rem;
    height: 1.4rem;
    line-height: 1.4rem;
    padding: 0 0.2rem;
    width: 35%;
    text-align: center;
    margin: 0.4rem 0 0 0; }

#bar {
  background-color: transparent; }

#bar span {
  background-color: rgba(0, 0, 0, 0.3); }

#bar:after {
  height: 0; }

.swiper-container {
  padding-bottom: 0; }
  .swiper-container .swiper-slide img {
    width: 100%; }

.swiper-container-horizontal {
  overflow: hidden; }

.swiper-container-horizontal > .swiper-pagination {
  bottom: 0.2rem; }

.notice-box {
  /*background: #fff;*/
  line-height: 2em;
  height: 2em;
  font-size: 0.8rem;
  color: #7D889E;
  padding: 0 0.5rem; }
  .notice-box em {
    font-style: normal;
    color: red;
    font-weight: bold; }

.home-wrap {
  padding: 0.5rem 0.5rem 5rem; }
  .home-wrap .fresh {
    width: 100%;
    margin-bottom: 0.5rem;
    overflow: hidden; }
    .home-wrap .fresh li {
      width: 48%;
      float: left;
      background: #fff; }
      .home-wrap .fresh li:first-child {
        margin-right: 4%; }
      .home-wrap .fresh li table tr td p {
        font-size: 0.6rem;
        line-height: 1.5em; }
      .home-wrap .fresh li table tr td:first-child {
        width: 40%;
        padding-right: 5%; }
      .home-wrap .fresh li table tr td svg {
        display: block;
        width: 60%;
        margin-left: 25%; }
      .home-wrap .fresh li table tr td h4 {
        font-size: 0.8rem;
        line-height: 1.5em; }
      .home-wrap .fresh li p {
        color: #C29A1E; }
  .home-wrap .defaut-wrap {
    padding: 0rem 1rem 1rem;
    color: #b7b8b7;
    text-align: center; }
    .home-wrap .defaut-wrap p {
      width: 9rem;
      margin: 0 auto; }
    .home-wrap .defaut-wrap .def-note {
      font-size: 0.8rem;
      text-align: center;
      font-weight: normal; }
    .home-wrap .defaut-wrap .icon {
      display: block;
      font-size: 3rem;
      width: 3rem;
      height: 3rem;
      margin: 0 auto; }

.company li {
  border: #E3E3E3 1px solid;
  margin-bottom: 0.5rem;
  background: -webkit-linear-gradient(left, #fff 45%, #E7E7E7);
  /* Safari 5.1 - 6.0 */
  background: -o-linear-gradient(right, #fff 45%, #E7E7E7);
  /* Opera 11.1 - 12.0 */
  background: -moz-linear-gradient(right, #fff 45%, #E7E7E7);
  /* Firefox 3.6 - 15 */
  background: linear-gradient(to right, #fff 45%, #E7E7E7);
  /* 标准的语法 */ }
  .company li table tr td {
    width: 36%;
    padding: 0 2%; }
    .company li table tr td:first-child {
      width: 22%; }
    .company li table tr td h4 {
      font-size: 0.8rem;
      line-height: 2em; }
    .company li table tr td p {
      font-size: 0.65rem;
      color: #5B5B5B;
      height: 3em;
      overflow: hidden; }
    .company li table tr td img {
      display: block;
      width: 100%; }
