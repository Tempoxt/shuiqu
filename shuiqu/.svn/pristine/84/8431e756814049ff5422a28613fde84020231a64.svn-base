/*添加提现账号*/
.content-addCash{
	table{
		width: 100%;
		height: 2rem;
		line-height: 2rem;
		background: #fff;
		margin-top: 0.3rem;
		tr{
			line-height: 2rem;
			td{
				font-size: 0.8rem;
				label{
					margin-left: 0.3rem;
					.icon-form-checkbox{
						width: 0.9rem;
						height: 0.9rem;
					}
				}
				u{
					text-decoration: none;
					margin-left: 0.3rem;
					font-size: 0.5rem;
					color:#A1A1A1;
				}
				.icon-49zhifubao{
					font-size: 1rem;
					margin-right: 0.25rem;
				}
				.icon-yinhang{
					font-size: 1.2rem;
					margin-right: 0.25rem;
				}
			}
			.pull-left{
				padding-left: 1rem;
			}
			.pull-right{
				padding-right: 1rem;
			}
		}
	}
	.new-form-list{
	    margin: 0.2rem 0 0;
	    font-size: 0.8rem;
	    .item-title.label{
	        width: auto;
	    }
	    input[type="text"]{
	        font-size: 0.7rem; 
	    }
	    input[type="password"]{
	        font-size: 0.7rem; 
	    }
	    .button.button-fill{
	    	font-size: 0.8rem;
	        width: 90%;
	        margin: 1rem auto 1rem;
	        line-height: 1.8rem;
	        height: 1.8rem;
	    }
	    .edit-forget{
	    	width: 90%;
	    	margin: 0 auto;
	    	a{
	    		color: #4C5B87;
	    		font-size: 0.7rem;
	    	}
	    }
	    ul{
	    	margin-top: 0.3rem;
	    	li{
	    		margin-right: 10px;
	    		margin-left: 10px;
	    		border-bottom: 1px solid #EDEDEC;
	    	}
	    	li:last-child{
	    		border-bottom: none;
	    	}
	    }
	}
	
}
