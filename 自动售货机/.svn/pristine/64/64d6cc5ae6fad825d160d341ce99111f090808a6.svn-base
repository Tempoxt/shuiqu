.mui-bar-tab{
	background: #FEFEFE;
	box-shadow: inset 0 0 0 0 rgba(0,0,0,0.16);
	border-top: 1px solid #E9E9E9;
}
.mui-bar-tab .mui-tab-item{
	color:#979797;
}
.mui-bar{
	-webkit-box-shadow: 0 0 0;
	box-shadow: 0 0 0;
}
.mui-fullscreen{
	height: 100%;
	background: #F8F8F8;
}
.mui-content{
	/*height: 100%;*/
	background: #F8F8F8;
	padding-bottom: 50px;
}
.mui-content .mui-table-view{
	margin-top: 7px;
}
