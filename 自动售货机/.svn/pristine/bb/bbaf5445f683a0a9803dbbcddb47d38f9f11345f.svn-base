
/*首页*/
.mui-segmented-control .mui-control-item.mui-active .cabinet-item .status{
	background: #2CC136;
}
.mui-content .mui-row .mui-col-sm-4.mui-col-xs-4 .mui-table-view-cell .mui-btn-theme{
	color: #fff;
	background: #2CC136;
}
.mui-segmented-control .mui-control-item.mui-active .cabinet-item .cabinet-num{
	background: #2CC136;
}
/*我的*/
.mui-content .my-info{
	height: 230px;
	width: 100%;
	background-image: linear-gradient(-129deg, #B4EC51 0%, #429321 100%);
	display: flex;
	flex-direction: column;
	align-items: center;
	justify-content: center;
}
.mui-bar-tab .mui-tab-item{
	color:#979797;
}
.mui-bar-tab .mui-tab-item.mui-active{
	color: #2CC136;
}
/*支付结果*/
.mui-content .footer-box .footer #call{
	background: #2CC136;
	color: #fff;
	border-radius: 4px;
	border: 0;
}
/*申述订单*/
.phone-box .getCode{
	width: 38%;
	height: 44px;
	border: 0;
	background:#2CC136;
	font-size: 16px;
	color: #FFFFFF;
	float: left;
	border-top-left-radius:0;
	border-bottom-left-radius:0;
}
.phone-box .getCode:active {
	color: #fff;
	border: 0;
	background-color: #2CC136!important; 
}
.phone-box .getCode.countdown{
	background: #999;
}
.phone-box .getCode.countdown:active{
	background-color: #999!important; 
}
.footer{
	width: 100%;
	flex: 1;
	display: flex;
	box-sizing: border-box;
	justify-content: center;
	align-items: flex-end;
	padding-bottom: 10px;
}
.footer .foot-btn-theme{
	width: 280px;
	height: 44px;
	padding: 0;
	margin: 0;
	font-size: 20px;
	color: #FFFFFF;
	background: #2CC136;
	border-radius: 8px;
	border: 0;
}