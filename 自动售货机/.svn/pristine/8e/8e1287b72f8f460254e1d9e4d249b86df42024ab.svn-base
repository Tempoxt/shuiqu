
body{
	background: #F8F8F8;
}
.mui-content{
	height: 100%;
	background: #F8F8F8;
}
.mui-content .content-box{
	padding-bottom:50px;
}
.mui-segmented-control{
	border: 0;
	border-radius: 0;
	height: 110px;
	background: #fff;
	padding: 11px 10px;
}
.mui-segmented-control .mui-control-item.mui-active{
	background-color: transparent;
}
.mui-segmented-control .mui-control-item{
	border-color: #E6E6E6;
	color: #fff;
}
.mui-segmented-control .mui-control-item .cabinet-item{
	width: 104px;
	margin: 0 auto;
	/*margin-left: 38px;*/
	position: relative;
}
.mui-segmented-control .mui-control-item .cabinet-item .cabinet{
	display: inline-block;
	background:url(/images/123.png);
	width: 56px;
	height: 75px;
	border-radius: 2px;
	position: relative;
    top: 6px;
    left: 0;
    
}
.mui-segmented-control .mui-control-item.mui-active .cabinet-item .cabinet{
	background:url(/images/1.png);
}
.mui-segmented-control .mui-control-item .cabinet-item .cabinet-num{
	display: inline-block;
	font-weight: bold;
	width: 34px;
	height: 34px;
	line-height: 34px;
	border-radius: 50%;
	background:#CACACA;
	position: relative;
	top: -26px;
}
.mui-segmented-control .mui-control-item .cabinet-item .status{
	width: 12px;
	height: 12px;
	border-radius: 50%;
	background: transparent;
	position: absolute;
	left: 0;
	z-index: 10;
}

.mui-bar-tab{
	background: #FEFEFE;
	box-shadow: inset 0 0 0 0 rgba(0,0,0,0.16);
	border-top: 1px solid #E9E9E9;
}
.mui-bar{
	-webkit-box-shadow: 0 0 0;
	box-shadow: 0 0 0;
}
.mui-content .mui-row{
	height: 199px;
	padding: 15px 8px;
	border-bottom: 7px solid #C7C7C7;
	position: relative;
}
.mui-content .mui-row .mui-col-sm-4.mui-col-xs-4{
	/*width: 110px;*/
	max-width: 30%;
	height: 162px;
	background: #FFFFFF;
	box-shadow: 0 6px 4px 0 rgba(185,185,185,0.50);
	border-radius: 8px;
	position: relative;
	z-index: 2;
}
.mui-content .mui-row .mui-col-sm-4:nth-of-type(1){
	margin-right: 5%;
}
.mui-content .mui-row .mui-col-sm-4:nth-of-type(2){
	margin-right: 5%;
}
.mui-content .mui-row .mui-col-sm-4.mui-col-xs-4 .mui-table-view-cell{
	list-style: none;
	display: flex;
	flex-direction: column;
	align-items: center;
	height:162px;
	padding: 0;
	
}
/*.mui-content .mui-row .mui-col-sm-4.mui-col-xs-4 .mui-table-view-cell.mui-active{
	background: none;
}*/
.mui-content .mui-row .mui-col-sm-4.mui-col-xs-4 .mui-table-view-cell .info{
	width: 100%;
	height: 123px;
	display: flex;
	padding-top: 7px;
	flex-direction: column;
	align-items: center;
	border-bottom: 1px solid #F8F8F8;
}
.mui-content .mui-row .mui-col-sm-4.mui-col-xs-4 .mui-table-view-cell .info img{
	width: 60px;
	height: 60px;
}
.mui-content .mui-row .mui-col-sm-4.mui-col-xs-4 .mui-table-view-cell .info .pname{
	width: 100%;
	text-align: center;
	font-size: 12px;
	color: #999999;
	margin-top: 8px;
	overflow: hidden;
	text-overflow:ellipsis;
	white-space: nowrap;
}
.mui-content .mui-row .mui-col-sm-4.mui-col-xs-4 .mui-table-view-cell .info .price{
	font-size: 14px;
	color: #333333;
	/*margin-bottom: 13px;*/
}

.mui-content .mui-row .mui-col-sm-4.mui-col-xs-4 .mui-table-view-cell .mui-btn-disabled{
	padding: 0;
	position: static;
	width: 57px;
	height: 24px;
	background: #666;
	border-radius: 4px;
	-webkit-transform:none;
	transform:none;
	margin-top: 7px;
	border: 0;
	color: #F8F8F8;
}
.mui-content .mui-row .bottom-decorate{
	position: absolute;
	bottom: 0;
	left: 0;
	width: 100%;
	height: 24px;
	background: #E4E4E4;
	border-top-left-radius:10px;
	border-top-right-radius:10px;
	z-index: 1;
}
.mui-content .mui-row .box-number{
	background:#E24A4A;
	width: 16px;
	height: 16px;
	border-radius: 50%;
	position: absolute;
	top: 3px;
	left: 3px;
	font-size: 12px;
	color: #F8F8F8;
	display: flex;
	justify-content: center;
	align-items: center;
	letter-spacing: 0;
	z-index: 10;
}
.mui-content .mui-row .mui-col-sm-4.mui-col-xs-4 .mui-table-view-cell .buy{
	padding: 0;
	position: static;
	width: 57px;
	height: 24px;
	border-radius: 4px;
	-webkit-transform:none;
	transform:none;
	margin-top: 7px;
	border: 0;
	color:#fff;
	background: #fff;
}