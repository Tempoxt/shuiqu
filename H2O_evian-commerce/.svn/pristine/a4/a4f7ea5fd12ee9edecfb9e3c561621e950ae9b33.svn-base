package com.eviano2o.util;

public class NumberUtil {
	/** 
     * 将浮点数小数，固定保留两位小数 
     */  
    public static String toFixedNumber(Double d) {  
        if (d == null) {  
            return "";  
        }  
        java.text.DecimalFormat df = new java.text.DecimalFormat("0.00");  
        return df.format(d).toString();
    }  
}
